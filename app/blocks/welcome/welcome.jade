mixin welcome()
    section.welcome&attributes(attributes)
        .welcome__header
            img.welcome__logo(src="img/logo.png" alt="logo")
            button.welcome__open
                img.welcome__open-img(src="img/open.png" alt="open")
        img.welcome__lamp(src="img/lamp.png" alt="lamp")
        .welcome__container
            img.welcome__title(src="img/title.svg" alt="title")
            h3.welcome__subtitle Design